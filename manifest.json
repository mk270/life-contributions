{
    "name": "Contributions Come To Life",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Make your Github Contributions come to Life.",

    "content_scripts": [
        {
          "matches": ["https://github.com/*"],
	      "js": ["life.js"]
        }
    ],

  "permissions": [
	"tabs", "http://*/*"
  ]
}
